#include <bits/stdc++.h>
using namespace std;
/**
* IDEA: dp[mask][i] is a subset of nodes represented by the binary representation of mask, 
        which ends at i-th node
*/
 
vector<int> gr[25];
int n, m, mod = 1e9 + 7,dp[1<<21][25];
 
 
int TSP(int mask, int s){
    if(__builtin_popcount(mask) == n){
        return 1;
    }
    if(s == n){
        return 0;
    }
    if(dp[mask][s] != -1){
        return dp[mask][s];
    }
 
    int ans = 0;
    dp[mask][s] = 0;
    for(int  v : gr[s]){
        if(mask & (1 << v)){
            continue;
        }
        dp[mask][s] = (dp[mask][s] + TSP((mask | (1 << v)), v)) % mod;
    }
    return dp[mask][s];
}
 
int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
 
    // freopen("main.inp","r",stdin);
    // freopen("main.out","w",stdout);
 
    cin>>n>>m;
 
    while(m--){
        int u,v;
        cin>>u>>v;
        gr[u].push_back(v);
    }
 
    memset(dp, -1, sizeof(dp));
    cout<<TSP((1 << 1), 1); 
 
    return 0;
}
