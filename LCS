#include <bits/stdc++.h>
using namespace std;

int dp[3005][3005];
string a,b;

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    string temp = "";
    cin>>a>>b;

    a = "~" + a;
    b = "!" + b;

    for(int i=0;i<a.size();i++){
        for(int j=0;j<b.size();j++){
            dp[i][j] = 0;
            if(a[i] == b[j]){
                dp[i][j] = 1;
                if(i > 0 && j > 0){
                    dp[i][j] += dp[i-1][j-1];
                }
            }
            else{
                if(i > 0 && j > 0){
                    dp[i][j] = max(dp[i-1][j],dp[i][j-1]);
                }
                else if(i > 0){
                    dp[i][j] = dp[i-1][j];
                }
                else if(j > 0){
                    dp[i][j] = dp[i][j-1];
                }
            }
        }
    }

    int i=a.size()-1,j=b.size()-1;
    string ans = "";

    while(i >= 0 && j >= 0){
        if(a[i] == b[j]){
            ans = a[i] + ans;
            i--;
            j--;
        }
        else if(dp[i-1][j] > dp[i][j-1]){
            i--;
        }
        else{
            j--;
        }
    }

    cout<<ans;
    return 0;
}
