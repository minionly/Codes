#include <bits/stdc++.h>
using namespace std;

long long w[105],v[105],dp[100005];

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n,m;

    cin>>n>>m;

    for(int i=1;i<=n;i++){
        cin>>w[i]>>v[i];
    }

    for(int i=1;i<=n;i++){
        for(int j=m;j>=w[i];j--){
            dp[j] = max(dp[j],dp[j - w[i]] + v[i]);
        }
    }

    cout<<dp[m];
    
    return 0;
}
