#include <bits/stdc++.h>
using namespace std;

int dp[2005][2005];

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    freopen("main.inp","r",stdin);
    freopen("main.out","w",stdout);

    string s;
    cin>>s;

    for(int i=s.size()-2;i>=0;i--){
        for(int j=i+1;j<s.size();j++){
            if(s[i] == s[j]){
                dp[i][j] = dp[i+1][j-1];
            }
            else{
                dp[i][j] = min(dp[i+1][j],dp[i][j-1]) + 1;
            }
        }
    }

    int i=0,j=s.size()-1;
    // tracing for deletion
    while(dp[i][j] > 0){
        if(s[i] == s[j]){
            i++;
            j--;
        }
        else{
            if(dp[i+1][j] <= dp[i][j+1]){
                s[i] = '0';
                i++;
            }
            else{
                s[j] = '0';
                j--;
            }
        }
    }

    for(char c : s){
        if(c != '0'){
            cout<<c;
        }
    }

    return 0;
}
